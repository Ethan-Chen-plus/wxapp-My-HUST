            </view>
            <view class="newest-list">
                <navigator url="details4/details4">
                    <image src="http://qungz.photo.store.qq.com/qun-qungz/jVL04mTYBBHh3.YcjtAIxw!!/V5bCQA3ODY2MDkwNjYbVBdhWjbMMA!!/800?w5=640&h5=640&rf=viewer_421"></image>
